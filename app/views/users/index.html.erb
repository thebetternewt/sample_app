<% provide(:title, 'All Users')%>
<h1>All Users</h1>

<%= will_paginate @users, renderer: BootstrapPagination::Rails %>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size: 100 %>
      <%= link_to user.name, user %>
    </li>
  <% end %>
</ul>

<%= will_paginate @users, renderer: BootstrapPagination::Rails %>
